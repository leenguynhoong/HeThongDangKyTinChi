
@model PagedList.IPagedList<BootStrap4.Models.Dang_Ky_Tin.DangKyTinModel>
@{
    ViewBag.Title = "Hủy đăng ký";


}

<div class="modal-content" style="width:500px;height:300px">
    <div class="modal-header">
        <h2>Mai co chac chan muon huy danng ky mon nay khum</h2>
        <button id="btnClose" type="button" class="close" data-dismiss="modal">&times;</button>
    </div>
    <div class="modal-body">
        <form id="formdata" method="POST">
            <div class="form-group">
                <label>Ma mon:</label>
                <input type="text" id="popUpHuyMaMon" class="form-control form-control-user">
                <label>Ten mon hoc:</label>
                <input type="text" id="popUpHuyTenMon" class="form-control form-control-user">
                @*<input type="hidden" id="PopUpDelMaMon" />*@
            </div>
        </form>
    </div>
    <div class="modal-footer">
    
        <button type="button" class="btn btn-danger  button button4" data-save="modal" id="Update" onclick="HuyDK()">Hủy đăng ký</button>
        <button type="button" class="btn btn-primary button button4" id="Cancel">Cancel</button>
    </div>

</div>

