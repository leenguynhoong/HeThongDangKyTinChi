
@model PagedList.IPagedList<BootStrap4.Models.Mon_Hoc.MonHocModel>
@{
    ViewBag.Title = "PopUpRegis";
}
<div class="modal-content" style="width:500px;height:300px">
    <div class="modal-header">
        <h2>Mai co chac chan muon dang ky khum</h2>
        <button id="btnClose" type="button" class="close" data-dismiss="modal">&times;</button>
    </div>
    <div class="modal-body">
        <form id="formdata" method="POST">
            <div class="form-group">
                <label>Ma mon hoc:</label>
                <input type="text" id="PopUpMaMon" class="form-control form-control-user">
                <label>Ten mon hoc:</label>
                <input type="text" id="PopUpTenMon" class="form-control form-control-user">
                <input type="hidden" id="PopUpMaMon">
            </div>
        </form>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-danger  button button2" data-save="modal" id="Update" onclick="RegisMhYes()">Yes</button>
        <button type="button" class="btn btn-primary button button4" id="Cancel">No</button>
    </div>
</div>

